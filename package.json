{
  "name": "portfolio",
  "homepage": "https://arminas-a.github.io/codewhile",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^4.15.0",
    "gh-pages": "^1.1.0",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-burger-menu": "^2.3.0",
    "react-dom": "^16.2.0",
    "react-form": "^2.16.1",
    "react-instafeed": "^0.4.0",
    "react-particles-js": "^2.1.1",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.0",
    "react-scripts": "1.0.17",
    "react-transition-group": "^1.2.1",
    "rrc": "^0.10.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass-chokidar ./src/styles/scss -o ./src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/scss -o ./src/styles/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2"
  }
}
